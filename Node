package week8;

//Most taken from in class notes

public class Node<T> {

	private T data;
	private Node<T> next;
	
	public Node() {
		// TODO Auto-generated constructor stub
		data = null;
		next = null;
	}
	
	public Node(T data) {
		// TODO Auto-generated constructor stub
		this.data = data;
		next = null;
	}
	
	public Node(T data, Node<T> next) {
		// TODO Auto-generated constructor stub
		this.data = data;
		this.next = next;
	}

	public T getData() {
		return data;
	}

	public void setData(T data) {
		this.data = data;
	}

	public Node<T> getNext() {
		return next;
	}

	public void setNext(Node<T> next) {
		this.next = next;
	}
}
